{
  "name": "adapt-authoring-middleware",
  "version": "1.0.0",
  "description": "Express middleware to be added to the server",
  "homepage": "https://github.com/adapt-security/adapt-authoring-middleware",
  "license": "GPL-3.0",
  "type": "module",
  "main": "index.js",
  "repository": "github:adapt-security/adapt-authoring-middleware",
  "dependencies": {
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "bytes": "^3.1.2",
    "compression": "^1.7.4",
    "file-type": "^20.5.0",
    "formidable": "^3.5.1",
    "helmet": "^8.0.0",
    "lodash": "^4.17.21",
    "rate-limiter-flexible": "^7.3.0",
    "zipper": "github:adapt-security/zipper"
  },
  "peerDependencies": {
    "adapt-authoring-core": "^1.0.0"
  },
  "devDependencies": {
    "standard": "^17.1.0",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.5",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "conventional-changelog-eslint": "^3.0.9",
    "semantic-release": "^21.0.1",
    "semantic-release-replace-plugin": "^1.2.7"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "eslint"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "Chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
